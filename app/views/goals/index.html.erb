<h1>Goals</h1>

<h5>In progress:</h5>
<% @goals.unfinished.each do |goal| %>
    <%= link_to goal.name, goal_path(goal), class: "btn btn-info btn-lg" %>
    <br><br>
<% end %>

<h5>Accomplished!</h5>
<% @goals.finished.each do |goal| %>
    <%= link_to goal.name, goal_path(goal), class: "btn btn-outline-dark btn-lg" %>
    <br><br>
<% end %>


